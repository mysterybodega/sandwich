{
  "name": "sandwich-beta",
  "productName": "Sandwich Beta",
  "version": "0.2.3",
  "description": "Sandwich is an Electron app for combining PDFs and images together.",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts .",
    "test": "jest --env=jsdom",
    "test:watch": "jest --watchAll --env=jsdom"
  },
  "keywords": [],
  "author": {
    "name": "hachibu",
    "email": "raymondsohn@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/assets/images/sandwich-icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "sandwich"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/Renderer.tsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@electron-forge/cli": "6.0.0-beta.53",
    "@electron-forge/maker-deb": "6.0.0-beta.53",
    "@electron-forge/maker-rpm": "6.0.0-beta.53",
    "@electron-forge/maker-squirrel": "6.0.0-beta.53",
    "@electron-forge/maker-zip": "6.0.0-beta.53",
    "@electron-forge/plugin-webpack": "6.0.0-beta.53",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.3",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@types/react-sortable-hoc": "^0.7.1",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "babel-jest": "^26.3.0",
    "css-loader": "^4.3.0",
    "electron": "10.1.1",
    "eslint": "^7.9.0",
    "eslint-plugin-import": "^2.20.0",
    "file-loader": "^6.1.0",
    "fork-ts-checker-webpack-plugin": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jsdom": "^16.4.0",
    "node-loader": "^1.0.1",
    "postcss": "^7.0.32",
    "postcss-loader": "^4.0.1",
    "sass": "^1.26.10",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "electron-squirrel-startup": "^1.0.0",
    "filesize": "^6.1.0",
    "font-awesome": "^4.7.0",
    "normalize.css": "^8.0.1",
    "pdf-lib": "^1.10.0",
    "prop-types": "^15.5.7",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dropzone-uploader": "^2.11.0",
    "react-sortable-hoc": "^1.11.0"
  }
}
